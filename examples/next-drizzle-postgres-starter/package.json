{
  "name": "examples-trpc-next-drizzle-starter",
  "private": true,
  "scripts": {
    "db-seed": "tsx src/server/db/seed.ts",
    "dx:next": "run-s db-seed && next dev",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache src",
    "lint-fix": "pnpm lint --fix",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "typecheck": "tsc --noEmit"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@tanstack/react-query": "^5.80.3",
    "@trpc/client": "npm:@trpc/client",
    "@trpc/next": "npm:@trpc/next",
    "@trpc/react-query": "npm:@trpc/react-query",
    "@trpc/server": "npm:@trpc/server",
    "clsx": "^2.0.0",
    "drizzle-orm": "^0.44.7",
    "next": "^15.3.1",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^1.12.4",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.0",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.19.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.1.1",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "version": "11.7.2"
}
